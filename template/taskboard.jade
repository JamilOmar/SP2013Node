#mybutton(data-bind="ui:{type:'button'}") Hallo
div.page
    div.page-region
        div.page-region-content
            div.row
                div.span10(data-bind="foreach:tasks")
                    div.tile(data-bind="ui:{type:'draggable'}")
                        div.tile-content(data-bind="css:cssClass " )
                            h2(data-bind="text: owner")
                            h4(data-bind="text: title")
                            p(data-bind="text:description")
                            h5(data-bind="text:dueOn")